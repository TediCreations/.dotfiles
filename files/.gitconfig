[core]
	editor = emacs
	quotepath = false
[diff]
    prompt = false
	tool = meld
[merge]
	tool = meld
	conflictstyle = diff3
[alias]
    lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
    lg2 = log --all --graph --decorate --oneline
	st = status
	co = checkout
	ci = commit
	br = branch
	d = difftool
[mergetool]
	prompt = false
[web]
	browser = firefox
[instaweb]
    port = 4000
    browser = firefox
